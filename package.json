{
  "name": "ai-data-agent",
  "version": "1.0.0",
  "description": "AI-powered data analysis application",
  "main": "server/src/index.js",
"scripts": {
  "start": "node server/src/index.js",
  "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\"",
  "server": "cd server && nodemon src/index.js",
  "client": "cd client && npm start",
  "install-all": "npm install && cd client && npm install",
  "build": "cd client && npm run build"
}
,
  "keywords": [
    "ai",
    "data-analysis",
    "react",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
